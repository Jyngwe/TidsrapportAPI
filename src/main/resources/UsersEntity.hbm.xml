<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.UsersEntity" table="users" schema="public" catalog="dct2r6gnlhqdhu">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="email">
            <column name="email" sql-type="varchar(200)" length="200"/>
        </property>
        <property name="fullname">
            <column name="fullname" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="pass">
            <column name="pass" sql-type="varchar(50)" length="50"/>
        </property>
    </class>
</hibernate-mapping>